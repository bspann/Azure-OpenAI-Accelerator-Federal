      "source": [
        "#load message from cosmosdb\n",
        "cosmos.load_messages()\n",
        "cosmos.messages"
      ]
    },
    {
      "cell_type": "markdown",
      "id": "f5e30694-ae2a-47bb-a5c7-db51ecdbba1e",
      "metadata": {},
